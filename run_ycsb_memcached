#! /bin/bash
cd /home/sauron/YCSB/
/home/sauron/YCSB/bin/ycsb load memcached -P /home/sauron/YCSB/workloads/workloada -p "memcached.hosts=127.0.0.1" -p "memcached.port=11211"

/home/sauron/YCSB/bin/ycsb run memcached -P /home/sauron/YCSB/workloads/workloada -p "memcached.hosts=127.0.0.1" -p "memcached.port=11211" -threads 10 -p operationcount=99999999999999